<Window x:Class="Stolovie_Pribori__User_02.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stolovie_Pribori__User_02"
        mc:Ignorable="d"
        Title="ООО «Столовые приборы»" Height="450" Width="800" Icon="/Resources/icon.ico" MinWidth="700" MinHeight="400" MaxWidth="1920" MaxHeight="1080" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Background="AliceBlue">
                <Image Grid.RowSpan="2"></Image>
                <TextBlock x:Name="txtUserFullName" Grid.Row="0" HorizontalAlignment="Right" FontSize="20" VerticalAlignment="Center" Margin="0 0 10 0"></TextBlock>
                <Image HorizontalAlignment="Left" Height="45" Grid.Row="0" Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Source="/Resources/logo.png"/>
            </Grid>
            <Frame x:Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden" Grid.Row="1"></Frame>
        </Grid>
    </Grid>
</Window>
